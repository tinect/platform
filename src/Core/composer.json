{
    "name": "shopware/core",
    "type": "library",
    "description": "Shopware platform is the core for all Shopware ecommerce products.",
    "keywords": [
        "shopware",
        "shop"
    ],
    "homepage": "http://www.shopware.com",
    "license": "MIT",
    "autoload": {
        "files": [
            "Framework/Adapter/Doctrine/Patch/AbstractAsset.php",
            "Framework/Adapter/Twig/functions.php"
        ],
        "exclude-from-classmap": [
            "*/Test/"
        ],
        "psr-4": {
            "Shopware\\Core\\": ""
        }
    },
    "support": {
        "issues": "https://issues.shopware.com",
        "forum": "https://forum.shopware.com",
        "wiki": "https://developer.shopware.com",
        "docs": "https://developer.shopware.com",
        "chat": "https://slack.shopware.com"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.5.x-dev",
            "dev-trunk": "6.5.x-dev"
        }
    },
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-dom": "*",
