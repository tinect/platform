{
  "name": "sw-next-storefront",
  "version": "1.0.0",
  "private": true,
  "description": "Storefront Building Pipeline for Shopware Next",
  "keywords": [
    "next",
    "pipeline",
    "storefront",
    "sw"
  ],
  "license": "MIT",
  "scripts": {
    "development": "NODE_ENV=development webpack --config webpack.config.js",
    "check-modules": "are-you-es5 check . -r",
    "hot": "NODE_ENV=development MODE=hot webpack-dev-server --config webpack.config.js",
    "hot-proxy": "NODE_ENV=development MODE=hot node ./build/start-hot-reload.js",
    "lint": "eslint --config .eslintrc.js ./src --cache && stylelint --config stylelint.config.js ./src --cache",
    "lint-fix": "eslint --fix --config .eslintrc.js ./src && stylelint --fix --config stylelint.config.js ./src/scss",
    "lint:js": "eslint --config .eslintrc.js ./src",
    "lint:scss": "stylelint --config stylelint.config.js ./src --cache",
    "lint:scss-fix": "npm run lint:scss -- --fix",
    "production": "NODE_ENV=production webpack --config webpack.config.js",
    "unit": "jest --config jest.config.js --ci",
    "unit-watch": "jest --config jest.config.js --watch",
    "lighthouse": "node lighthouse-tests.js"
  },
  "dependencies": {
    "@lhci/cli": "0.9.0",
    "@popperjs/core": "2.11.6",
    "@shopware-ag/webpack-plugin-injector": "1.0.6",
    "are-you-es5": "2.1.2",
    "axios": "0.27.2",
    "bootstrap": "5.2.2",
    "deepmerge": "4.2.2",
    "expose-loader": "0.7.5",
    "flatpickr": "4.6.13",
    "friendly-errors-webpack-plugin": "1.7.0",
    "fs-extra": "10.1.0",
    "hammerjs": "2.0.8",
    "history": "4.10.1",
    "lighthouse": "9.2.0",
    "lodash": "4.17.21",
    "mini-css-extract-plugin": "0.8.0",
    "popper.js": "1.15.0",
    "puppeteer": "13.1.2",
    "query-string": "6.14.1",
    "stylelint-junit-formatter": "0.2.2",
    "terser-webpack-plugin": "4.2.3",
    "tiny-slider": "2.9.4",
    "webpack": "4.38.0",
    "webpackbar": "5.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-transform-object-assign": "7.2.0",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-typescript": "7.16.7",
    "@nuxtjs/friendly-errors-webpack-plugin": "2.1.0",
    "autoprefixer": "9.6.1",
    "babel-eslint": "10.0.2",
    "babel-loader": "8.0.6",
    "chokidar": "3.5.3",
    "css-loader": "3.1.0",
    "eslint": "7.10.0",
    "file-loader": "4.1.0",
    "html-loader-jest": "0.2.1",
    "jest": "24.8.0",
    "jest-junit": "6.3.0",
    "jest-plugin-clock": "2.9.0",
    "eslint-plugin-jest": "26.6.0",
    "postcss-loader": "3.0.0",
    "postcss-pxtorem": "4.0.1",
    "sass": "1.32.8",
    "sass-loader": "7.2.0",
    "style-loader": "0.23.1",
    "stylelint": "13.13.1",
    "stylelint-config-sass-guidelines": "8.0.0",
    "webpack-bundle-analyzer": "3.4.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "4.2.1",
    "xml2js": "0.4.23"
  }
}
