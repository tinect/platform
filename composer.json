{
    "name": "shopware/platform",
    "description": "The Shopware e-commerce core",
    "type": "library",
    "keywords": [
        "framework",
        "shopware"
    ],
    "homepage": "https://www.shopware.com",
    "license": "MIT",
    "replace": {
        "shopware/core": "self.version",
        "shopware/storefront": "self.version",
        "shopware/administration": "self.version",
        "shopware/elasticsearch": "self.version"
    },
    "support": {
        "issues": "https://issues.shopware.com",
        "forum": "https://forum.shopware.com",
        "wiki": "https://developer.shopware.com",
        "docs": "https://developer.shopware.com",
        "chat": "https://slack.shopware.com"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.5.x-dev",
            "dev-trunk": "6.5.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "process-timeout": 7200,
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true,
            "composer/package-versions-deprecated": true,
            "phpstan/extension-installer": true,
            "cweagans/composer-patches": true,
            "symfony/runtime": true
        }
    },
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-session": "*",
        "ext-simplexml": "*",
        "ext-sodium": "*",
        "ext-xml": "*",
        "ext-zip": "*",
        "ext-zlib": "*",
        "composer-runtime-api": "^2.0",
        "cocur/slugify": "4.0.0",
        "composer/composer": "^2.4.4",
        "composer/semver": "^3.2",
        "doctrine/annotations": "1.13.1",
        "doctrine/dbal": "~3.5",
        "doctrine/inflector": "~1.4.4",
        "tecnickcom/tcpdf": "6.4.4",
        "setasign/fpdi": "2.3.6",
        "opensearch-project/opensearch-php": "~2.0",
        "ezyang/htmlpurifier": "~4.16",
        "guzzlehttp/guzzle": "~7.4.5",
        "guzzlehttp/psr7": "^1.8.0",
        "jdorn/sql-formatter": "1.2.17",
        "lcobucci/jwt": "~4.1.5",
        "league/flysystem": "~3.10.3",
        "league/flysystem-async-aws-s3": "~3.10",
        "league/flysystem-google-cloud-storage": "~3.10",
        "league/flysystem-memory": "~3.10",
        "league/oauth2-server": "~8.3.2",
        "meyfa/php-svg": "~0.11.3",
        "nyholm/psr7": "1.4.0",
        "padaliyajay/php-autoprefixer": "1.3",
        "psr/cache": "~3.0.0",
        "psr/event-dispatcher": "1.0.0",
        "psr/http-factory": "1.0.1",
        "psr/http-message": "1.0.1",
        "psr/log": "1.1.2",
        "scssphp/scssphp": "v1.11.0",
        "sensio/framework-extra-bundle": "~6.2.1",
        "shopware/conflicts": "*",
        "shopwarelabs/dompdf": "1.0.3",
        "shyim/opensearch-php-dsl": "~1.0.1",
        "symfony/asset": "~6.1.0",
        "symfony/cache": "~6.1.0",
        "symfony/cache-contracts": "~3.1.0",
        "symfony/config": "~6.1.0",
        "symfony/console": "~6.1.0",
        "symfony/debug-bundle": "~6.1.0",
        "symfony/dependency-injection": "~6.1.0",
        "symfony/deprecation-contracts": "~3.1.0",
        "symfony/dotenv": "~6.1.0",
        "symfony/doctrine-messenger": "~6.1.0",
        "symfony/error-handler": "~6.1.0",
        "symfony/event-dispatcher": "~6.1.0",
        "symfony/event-dispatcher-contracts": "~3.1.0",
        "symfony/filesystem": "~6.1.0",
        "symfony/finder": "~6.1.0",
        "symfony/framework-bundle": "~6.1.0",
        "symfony/http-foundation": "~6.1.0",
        "symfony/http-kernel": "~6.1.0",
        "symfony/mailer": "~6.1.0",
        "symfony/messenger": "~6.1.0",
        "symfony/proxy-manager-bridge": "~6.1.0",
        "symfony/mime": "~6.1.0",
        "symfony/monolog-bridge": "~6.1.0",
        "symfony/monolog-bundle": "~3.8.0",
        "symfony/options-resolver": "~6.1.0",
        "symfony/process": "~6.1.0",
        "symfony/property-access": "~6.1.0",
        "symfony/property-info": "~6.1.0",
        "symfony/psr-http-message-bridge": "~2.1.3",
        "symfony/rate-limiter": "~6.1.0",
        "symfony/runtime": "~6.1.0",
        "symfony/routing": "~6.1.0",
        "symfony/security-core": "~6.1.0",
        "symfony/security-csrf": "~6.1.0",
        "symfony/serializer": "~6.1.0",
        "symfony/service-contracts": "~3.1.0",
        "symfony/stopwatch": "~6.1.0",
        "symfony/translation": "~6.1.0",
        "symfony/translation-contracts": "~3.1.0",
        "symfony/twig-bridge": "~6.1.0",
        "symfony/twig-bundle": "~6.1.0",
        "symfony/validator": "~6.1.0",
        "symfony/var-exporter": "~6.1.0",
        "symfony/yaml": "~6.1.0",
        "twig/intl-extra": "~3.3.5",
        "twig/string-extra": "~3.3.5",
        "twig/twig": "~3.4.3",
        "zircote/swagger-php": "~4.5.1",
        "squirrelphp/twig-php-syntax": "1.7.0"
    },
    "require-dev": {
        "ext-tokenizer": "*",
        "ext-xmlwriter": "*",
        "bamarni/composer-bin-plugin": "~1.4.1",
        "brianium/paratest": "^6.2",
        "dg/bypass-finals": "^1.3",
        "dms/phpunit-arraysubset-asserts": "^0.2.1",
        "fakerphp/faker": "^1.20",
        "johnkary/phpunit-speedtrap": "~4.0.0",
        "league/construct-finder": "^1.1",
        "maltyxx/images-generator": "~1.0.0",
        "mbezhanov/faker-provider-collection": "~2.0.1",
        "nikic/php-parser": "~4.13.2",
        "opis/json-schema": "~1.0.19",
        "phpbench/phpbench": "^1.2",
        "phpdocumentor/reflection-docblock": "^5.3.0",
        "phpstan/extension-installer": "^1.1.0",
        "phpstan/phpstan": "1.8.11",
        "phpstan/phpstan-doctrine": "1.3.21",
        "phpstan/phpstan-phpunit": "1.2.2",
        "phpstan/phpstan-symfony": "1.2.14",
        "phpunit/php-code-coverage": "~9.2.14",
        "phpunit/phpunit": "~9.5.17",
        "smalot/pdfparser": "~0.14.0",
        "symfony/browser-kit": "~6.1.0",
        "symfony/css-selector": "~6.1.0",
        "symfony/dom-crawler": "~6.1.0",
        "symfony/expression-language": "~6.1.0",
        "symfony/phpunit-bridge": "~6.1.0",
        "symfony/var-dumper": "~6.1.0",
        "symfony/web-profiler-bundle": "~6.1.0"
    },
    "conflict": {
        "monolog/monolog": "^1.0",
        "enqueue/enqueue": "<0.10.16"
    },
    "scripts": {
        "post-install-cmd": [
            "@composer bin all install --ansi"

        ],
        "post-update-cmd": [
            "@composer bin all update --ansi"
        ],
        "hook:pre-commit": [
            "bin/pre-commit"
        ],
        "hook:pre-commit:install": [
            "ln -sf $PWD/bin/pre-commit .git/hooks/pre-commit"
        ],
        "hook:pre-push": [
            "bin/pre-commit"
        ],
        "hook:pre-push:install": [
            "ln -sf $PWD/bin/pre-push .git/hooks/pre-push"
        ],
        "init:db": [
            "@php bin/console system:install --drop-database --basic-setup --force --no-assign-theme"
        ],
        "init:testdb": [
            "FORCE_INSTALL=true vendor/bin/phpunit --group=none"
        ],
        "init:js": [
            "@npm:admin clean-install",
            "@npm:storefront clean-install"
        ],
        "setup": [
            "@composer install -o",
            "@composer install -d src/Recovery",
            "mkdir -p config/jwt || true",
            "mkdir -p custom/plugins || true",
            "@init:db",
            "@init:js",
            "@build:js",
            "@php bin/console theme:compile",
            "@php bin/console theme:change --all Storefront",
            "@php bin/console assets:install"
        ],
        "reset": [
            "@init:db",
            "@build:js",
            "@php bin/console theme:compile",
            "@php bin/console theme:change --all Storefront",
            "@php bin/console assets:install"
        ],
        "npm:admin:bin": "export PROJECT_ROOT=\"$PWD\"; cd src/Administration/Resources/app/administration; export PATH=\"$PWD/node_modules/.bin/:$PATH\"; \"$PROJECT_ROOT\"/bin/exec-with-env ",
        "npm:admin": "@npm:admin:bin npm",
        "npm:storefront:bin": "export PROJECT_ROOT=\"$PWD\"; cd src/Storefront/Resources/app/storefront; export PATH=\"$PWD/node_modules/.bin/:$PATH\"; \"$PROJECT_ROOT\"/bin/exec-with-env ",
        "npm:storefront": "@npm:storefront:bin npm",
        "npm:component-library:bin": "export PROJECT_ROOT=\"$PWD\"; cd src/Administration/Resources/app/administration/build/nuxt-component-library; export PATH=\"$PWD/node_modules/.bin/:$PATH\"; \"$PROJECT_ROOT\"/bin/exec-with-env ",
        "npm:component-library": "@npm:component-library:bin npm",
        "npm:e2e:bin": "export CYPRESS_shopwareRoot=\"$PWD\"; cd tests/e2e; export PATH=\"$PWD/node_modules/.bin/:$PATH\"; \"$CYPRESS_shopwareRoot\"/bin/exec-with-env ",
        "npm:e2e": "@npm:e2e:bin npm",
        "npm:e2e:admin:bin": "@npm:e2e:bin ",
        "npm:e2e:admin": "@npm:e2e:admin:bin npm",
        "npm:e2e:recovery:bin": "@npm:e2e:bin ",
        "npm:e2e:recovery": "@npm:e2e:recovery:bin npm",
        "npm:e2e:storefront:bin": "@npm:e2e:bin ",
        "npm:e2e:storefront": "@npm:e2e:storefront:bin npm",
        "e2e:sdk-plugin:fixture-to-plugin": [
            "rm -rf $PWD/custom/plugins/TestPlugin;",
            "cp -a $PWD/tests/e2e/cypress/fixtures/TestPlugin/. $PWD/custom/plugins/TestPlugin/"
        ],
        "e2e:sdk-plugin:plugin-to-fixture": [
            "rm -rf $PWD/tests/e2e/cypress/fixtures/TestPlugin;",
            "cp -a $PWD/custom/plugins/TestPlugin/. $PWD/tests/e2e/cypress/fixtures/TestPlugin/"
        ],
        "e2e:sdk-plugin:prepare-plugin": [
            "cd $PWD/custom/plugins/TestPlugin/src/Resources/app/administration/; npm install;",
            "bin/console plugin:refresh;",
            "bin/console plugin:install --activate TestPlugin;",
            "@build:js:admin"
        ],
        "build:js:admin": [
            "@php bin/console bundle:dump",
            "@npm:admin run build",
            "@php bin/console assets:install"
        ],
        "build:js:component-library": [
            "@npm:component-library run generate"
        ],
        "build:js:storefront": [
            "@php bin/console feature:dump",
            "@npm:storefront run production",
            "cd src/Storefront/Resources/app/storefront && node copy-to-vendor.js"
        ],
        "build:js": [
            "@php bin/console bundle:dump",
            "@php bin/console feature:dump",
            "@build:js:admin",
            "@build:js:storefront"
        ],
        "watch:admin": [
            "Composer\\Config::disableProcessTimeout",
            "@php bin/console feature:dump",
            "@php bin/console bundle:dump",
            "@npm:admin run dev"
        ],
        "watch:storefront": [
            "Composer\\Config::disableProcessTimeout",
            "@php bin/console feature:dump",
            "@php bin/console theme:dump",
            "@npm:storefront run hot-proxy"
        ],
        "stylelint:admin": "@npm:admin run lint:scss",
        "stylelint:storefront": "@npm:storefront run lint:scss",
        "stylelint": [
            "@stylelint:admin src/**/*.scss",
            "@stylelint:storefront src/**/*.scss"
        ],
        "eslint:admin": "@npm:admin run lint",
        "eslint:admin:fix": "@npm:admin run lint:fix",
        "eslint:storefront": "@npm:storefront:bin eslint --config .eslintrc.js --ext .js,.vue",
        "eslint": [
            "@eslint:admin",
            "@eslint:storefront"
        ],
        "ecs": "ecs check src tests",
        "ecs-fix": "ecs check --fix src tests",
        "lint:changelog": "@php bin/shopware changelog:check",
        "lint:snippets": "@php bin/shopware snippets:validate",
        "lint": [
            "@stylelint",
            "@eslint",
            "@ecs",
            "@lint:changelog",
            "@lint:snippets"
        ],
        "phpstan": [
            "@php src/Core/DevOps/StaticAnalyze/PHPStan/phpstan-bootstrap.php",
            "phpstan analyze --memory-limit=2G"
        ],
        "bc-check": [
            "@php vendor-bin/roave-backward-compatibility-check/bin/verify-version.php",
            "roave-backward-compatibility-check --from=$(git tag -l --sort -version:refname | head -n 1)"
        ],
        "static-analyze": [
            "@phpstan src/"
        ],
        "phpunit": [
            "phpunit --exclude-group=quarantined"
        ],
        "phpunit:quarantined": [
            "phpunit --group=quarantined"
        ],
        "paratest": [
            "paratest --configuration phpunit.xml.dist --testsuite=paratest --exclude-group=skip-paratest,needsWebserver,quarantined --processes $(getconf _NPROCESSORS_ONLN)"
        ],
        "locust:init": "php src/Core/DevOps/Locust/setup.php",
        "locust:run": "cd src/Core/DevOps/Locust && locust",
        "init:e2e": "@npm:e2e clean-install",
        "init:e2e:admin": "@npm:e2e:admin clean-install",
        "init:e2e:storefront": "@npm:e2e:storefront clean-install",
        "init:e2e:recovery": "@npm:e2e:recovery clean-install",
        "e2e:setup": [
            "@putenv APP_ENV=e2e",
            "@setup",
            "@e2e:prepare"
        ],
        "phpbench": "phpbench run --report=compressed",
        "e2e:prepare": [
            "@putenv APP_ENV=e2e",
            "@putenv CYPRESS_localUsage=1",
            "@e2e:sdk-plugin:fixture-to-plugin",
            "@e2e:sdk-plugin:prepare-plugin",
            "@php bin/console e2e:dump-db"
        ],

        "e2e:cypress": [
            "export APP_ENV=e2e; export CYPRESS_localUsage=1; export CYPRESS_shopwareRoot=\"$PWD\"; cd tests/e2e; export PATH=\"$PWD/node_modules/.bin/:$PATH\"; \"$CYPRESS_shopwareRoot\"/bin/exec-with-env cypress"
        ],
        "e2e:percy": [
            "export APP_ENV=e2e; export CYPRESS_localUsage=1; export CYPRESS_shopwareRoot=\"$PWD\"; cd tests/e2e; export PATH=\"$PWD/node_modules/.bin/:$PATH\"; \"$CYPRESS_shopwareRoot\"/bin/exec-with-env percy"
        ],

        "e2e:admin:cypress": [
            "@e2e:cypress"
        ],
        "e2e:admin:percy": [
            "@e2e:percy"
        ],
        "e2e:storefront:cypress": [
            "@e2e:cypress"
        ],
        "e2e:storefront:percy": [
            "@e2e:percy"
        ],
        "e2e:recovery:cypress": [
            "@e2e:cypress"
        ],
        "e2e:recovery:percy": [
            "@e2e:percy"
        ],
        "e2e:admin:run": [
            "@e2e:admin:cypress run --browser chrome --headless --spec \"cypress/integration/administration/**/*\""
        ],
        "e2e:storefront:run": [
            "@e2e:storefront:cypress run --browser chrome --headless --spec \"cypress/integration/storefront/**/*\""
        ],
        "e2e:recovery:run": [
            "@e2e:recovery:cypress run --browser chrome --headless --spec \"cypress/integration/recovery/**/*\""
        ],
        "e2e:open": [
            "Composer\\Config::disableProcessTimeout",
            "@e2e:cypress open"
        ],
        "e2e:admin:open": [
            "Composer\\Config::disableProcessTimeout",
            "@e2e:admin:cypress open"
        ],
        "e2e:storefront:open": [
            "Composer\\Config::disableProcessTimeout",
            "@e2e:storefront:cypress open"
        ],
        "e2e:recovery:open": [
            "Composer\\Config::disableProcessTimeout",
            "@e2e:recovery:cypress open"
        ],
        "framework:schema:dump": "@php bin/console framework:schema -s 'entity-schema' src/Administration/Resources/app/administration/test/_mocks_/entity-schema.json",
        "admin:unit": [
            "Composer\\Config::disableProcessTimeout",
            "@framework:schema:dump",
            "@npm:admin run unit"
        ],
        "admin:unit:watch": [
            "Composer\\Config::disableProcessTimeout",
            "@framework:schema:dump",
            "@npm:admin run unit-watch"
        ],
        "admin:create:test": "@npm:admin run create-test",
        "storefront:unit": [
            "@npm:storefront run unit"
        ],
        "storefront:unit:watch": [
            "Composer\\Config::disableProcessTimeout",
            "@npm:storefront run unit-watch"
        ]
    },
    "autoload": {
        "files":[
            "src/Core/Framework/Adapter/Doctrine/Patch/AbstractAsset.php",
            "src/Core/Framework/Adapter/Twig/functions.php"
        ],
        "psr-4": {
            "Shopware\\Administration\\": "src/Administration/",
            "Shopware\\Docs\\": "src/Docs/",
            "Shopware\\Storefront\\": "src/Storefront/",
            "Shopware\\Core\\": "src/Core/",
            "Shopware\\Elasticsearch\\": "src/Elasticsearch/"
        },
        "exclude-from-classmap": [
            "src/**/Test/",
            "tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Shopware\\Storefront\\Test\\": "src/Storefront/Test/",
            "Shopware\\Administration\\Test\\": "src/Administration/Test",
            "Shopware\\Recovery\\Test\\": "src/Recovery/Test",
            "Shopware\\Docs\\Test\\": "src/Docs/Test",
            "Shopware\\Core\\Content\\Test\\": "src/Core/Content/Test/",
            "Shopware\\Core\\Framework\\Test\\": "src/Core/Framework/Test/",
            "Shopware\\Core\\Migration\\Test\\": "src/Core/Migration/Test/",
            "Shopware\\Core\\Maintenance\\Test\\": "src/Core/Maintenance/Test/",
            "Shopware\\Core\\DevOps\\Test\\": "src/Core/DevOps/Test/",
            "Shopware\\Core\\System\\Test\\": "src/Core/System/Test/",
            "Shopware\\Core\\Checkout\\Test\\": "src/Core/Checkout/Test/",
            "Shopware\\Core\\Profiling\\Test\\": "src/Core/Profiling/Test/",
            "Shopware\\Core\\Test\\": "src/Core/Test/",
            "Shopware\\Tests\\Unit\\": "tests/unit/php/",
            "Shopware\\Tests\\Integration\\": "tests/integration/php/",
            "Shopware\\Tests\\Bench\\": "tests/performance/bench/",
            "Shopware\\Tests\\Migration\\": "tests/migration/"
        }
    }
}
